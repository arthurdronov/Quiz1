@model Quiz.Application.DTOs.UserDTO

<h1>Create user</h1>

<form method="post">
	<div class="mb-3">
		<label asp-for="Name"></label>
		<input asp-for="Name" type="text"/>
	</div>

	<div class="mb-3">
		<label asp-for="Age"></label>
		<input asp-for="Age" type="number" />
	</div>

	<button type="submit">Create</button>
</form>